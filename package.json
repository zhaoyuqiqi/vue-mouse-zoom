{
  "name": "vue-mouse-zoom",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "types": "dist/types/index.d.ts",
  "main": "dist/cjs/vue-mouse-zoom.js",
  "module": "dist/es/vue-mouse-zoom",
  "exports": {
    ".": {
      "types": "./dist/types/index.d.ts",
      "require": "./dist/cjs/vue-mouse-zoom.js",
      "import": "./dist/es/vue-mouse-zoom.js"
    },
    "./style": {
      "require": "./dist/style.css",
      "import": "./dist/style.css"
    }
